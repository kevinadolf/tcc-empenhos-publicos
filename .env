# API do TCE-RJ
TCE_API_BASE_URL=https://dados.tcerj.tc.br/api/v1
TCE_API_PAGE_SIZE=100
# Define número máximo de páginas durante desenvolvimento (None para ilimitado)
# TCE_API_MAX_PAGES=2
# Define anos a serem coletados (separados por vírgula). Vazio busca dados gerais.
TCE_API_YEARS=2024
# Limita a quantidade total de registros (0 ou vazio para ilimitado)
TCE_API_MAX_RECORDS=20
# Tempo em segundos para reutilizar o grafo carregado
GRAPH_CACHE_TTL_SECONDS=600

# Habilita chamadas reais à API (false usa dados de exemplo)
ENABLE_LIVE_FETCH=true
